{
    "ini": {
        "g": 0.008
    },
    "ps_param_space": {
        "tau": 10.0,
        "E": 0.0
    },
    "param_space": {
        "n_orn": "eval 1: params['neuron_populations']['or']['n']",
        "n_trg": "eval 1: params['neuron_populations']['pn']['n']",
        "n_pre": 12
    },
    "matrix_type": "SPARSE_GLOBALG",
    "w_update_model": "StaticPulse",
    "connectivity": {
        "name": "orn_al_type_specific",
        "type": "CustomSparse",
        "col_build_code": [
            "if (c == 0) {",
            "$(endCol);",
            "}",
            "const unsigned int glo= $(id_post)/((unsigned int) $(n_trg));",
            "const unsigned int offset= $(n_orn)*glo;",
            "const unsigned int tid= $(gennrand_uniform)*$(n_orn);",
            "$(addSynapse, offset+tid+$(id_pre_begin));",
            "c--;"
        ],
        "param_names": [
            "n_orn",
            "n_trg",
            "n_pre"
        ],
        "col_build_state_vars": [
            [
                "c",
                "unsigned int",
                "$(n_pre)"
            ]
        ],
        "calc_max_col_len_func": "eval 1: create_cmlf_class(lambda num_pre, num_post, pars: int(pars[2]))()"
    }
}