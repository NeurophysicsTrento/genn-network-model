{
    "ini": {
        "g": 2e-05,
        "requires_initing": true,
        "param_space": {
            "n_ln": "eval 1: params['n']['LNs']",
            "g": 2e-05
        },
        "connectivity": {
            "name": "lns_lns_conn_init",
            "type": "CustomInit",
            "param_names": [
                "n_ln",
                "g"
            ],
            "var_init_code": [
                "const unsigned int nln= (unsigned int) $(n_ln);",
                "$(value)=($(id_pre)/nln == $(id_post)/nln) ? 0.0 : $(g);"
            ]
        }
    },
    "postsyn": {
        "tau": 20.0,
        "E": -80.0
    },
    "matrix_type": "DENSE_INDIVIDUALG",
    "w_update_model": "StaticPulse"
}